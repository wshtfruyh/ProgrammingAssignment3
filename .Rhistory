pollutantmean <- function (directory, pollutant, id = 1:332) {
polfiles <- list.files(path = directory, pattern = ".csv",
full.names = TRUE)
values <- numeric()
for(i in id) {
data <- read.csv(polfiles[i])
values <- c(values, data[[pollutant]])
}
mean(values, na.rm = TRUE)
}
