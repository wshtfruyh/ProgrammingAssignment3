complete <- function(directory, id = 1:332) {
polcomp <- list.files(path = directory, pattern = ".csv",
file.names = TRUE)
values <- numeric()
for(i in id) {
data <- read.csv(polcomp[i])
nobs <- rbind(values, complete.cases(data))
}
dataframe <- c(id, nobs)
print(dataframe)
}
